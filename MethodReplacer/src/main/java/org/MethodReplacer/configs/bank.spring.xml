    </bean>
    <bean id="sbi" class="org.MethodReplacer.beans.SBI_Bank">
         <property name="id" value="5669879" />
         <property name="name" value="SBI bank" />
         <property name="address" value="South ALBAMA, USA" />
         <property name="netAmount" value="50000" />
    </bean>
    
     <bean id="u1" class="org.MethodReplacer.beans.User">
         <property name="id" value="12389" />
         <property name="name" value="Maharaja" />
         <property name="address" value="Bangalore" />
    </bean>
